{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dbserver",
            "program": "${workspaceFolder}/src/back/dbserver.ts",
            "runtimeExecutable": "ts-node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "front in Chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "front in Chrome (with URL)",
            "url": "http://localhost:3000/launch/54/route/339/flight/53358",
            "webRoot": "${workspaceFolder}"
        }
    ]
}